[application]

config/name="Jump-and-Stuff"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Autoloads/Globals.gd"
EventBus="*res://Autoloads/Signals.gd"
CheckpointSystem="*res://Autoloads/CheckpointSystem.gd"

